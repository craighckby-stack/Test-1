{
  "specification_version": "1.1.0",
  "engine_identifier": "MEE_V1.0",
  "contract_dependency_ref": {
    "contract_id": "MEC",
    "required_version": "1.1"
  },
  "data_io_schema": {
    "inputs": [
      {
        "id": "L4_Provenance_Log_Integrity_Score",
        "type": "float",
        "range": "[0.0, 1.0]",
        "description": "Normalized trust score derived from upstream logging integrity checks."
      },
      {
        "id": "Estimated_Resource_Consumption_Delta",
        "type": "integer",
        "unit": "Joule/sec",
        "description": "Predicted change in computational overhead post-execution."
      },
      {
        "id": "Historical_Performance_Baseline",
        "type": "float",
        "description": "Moving average of execution success rate (normalized)."
      }
    ],
    "outputs": [
      "S-01_Efficacy_Score",
      "S-02_Risk_Score"
    ]
  },
  "metric_definitions": {
    "S-01_Efficacy_Score": {
      "purpose": "Measures net predicted utility gain based on mandate alignment.",
      "output_type": "float",
      "computation_ref": {
        "module_id": "QUANT_EFFICACY",
        "version": "1.0.1",
        "executor_type": "PythonScriptRunner"
      },
      "threshold_integration": {
        "target_parameter": "P-01_PASS_CRITERIA",
        "contribution_model": "LINEAR_WEIGHTED",
        "weight": 0.65
      }
    },
    "S-02_Risk_Score": {
      "purpose": "Measures system exposure, volatility, and probability of cascading failure.",
      "output_type": "float",
      "computation_ref": {
        "module_id": "QUANT_VOLATILITY",
        "version": "3.2.0",
        "executor_type": "RustService"
      },
      "threshold_integration": {
        "target_parameter": "P-01_PASS_CRITERIA",
        "contribution_model": "HARD_CONSTRAINT",
        "constraint_type": "MAXIMUM_VALUE",
        "value": 0.35
      }
    }
  },
  "compliance_audit_mandate": {
    "required": true,
    "audit_level": "L5_REGISTRATION_PREREQUISITE",
    "scope": "Mandated execution environment audit"
  }
}