{
  "AAM_ID": "GSEP-C-AAM",
  "AAM_VERSION": "V94.1-GSEP-C",
  "description": "Defines the mandatory I/O contract and operational sequence for all Governance Actors across the GSEP-C pipeline stages (S00-S14). Used for pre-execution dependency resolution and artifact standardization.",
  "artifact_registry": {
    "CSR": {
      "name": "Configuration Snapshot Receipt",
      "schema_version": "1.2",
      "integrity_target": "GAX III Integrity Check"
    },
    "ECVM": {
      "name": "Context Validation Map",
      "schema_version": "1.0",
      "integrity_target": "GAX II Compliance Check"
    },
    "TEMM": {
      "name": "Transition Efficacy Measure",
      "schema_version": "2.1",
      "integrity_target": "GAX I Efficiency Check"
    }
  },
  "stages": [
    {
      "stage_id": "S01",
      "actor_id": "CRoT Agent",
      "operation_mandate": "Capture foundational execution state data.",
      "consumes": [],
      "produces": ["CSR"]
    },
    {
      "stage_id": "S07",
      "actor_id": "EMS",
      "operation_mandate": "Validate current execution context against CRoT outputs.",
      "consumes": ["CSR"],
      "produces": ["ECVM"]
    },
    {
      "stage_id": "S08",
      "actor_id": "EMS",
      "operation_mandate": "Measure operational deviation and efficacy of context transition.",
      "consumes": ["CSR", "ECVM"],
      "produces": ["TEMM"]
    },
    {
      "stage_id": "S11",
      "actor_id": "GAX Executor",
      "operation_mandate": "Aggregate validated metrics and state data to execute finality resolution.",
      "consumes": ["CSR", "ECVM", "TEMM"],
      "produces": [],
      "finality_status": "P-01 Finality Commitment"
    }
  ]
}