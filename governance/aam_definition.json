{
  "AAM_ID": "GSEP-C-AAM",
  "AAM_VERSION": "V94.2-AAM-Standardized",
  "description": "Defines the mandatory I/O contract and operational sequence for all critical Governance Actors (G-Actors) within the GSEP-C pipeline. Includes decoupled integrity definitions for standardized validation.",
  "integrity_definitions": {
    "GAX_I_EFFICIENCY": {
      "name": "GAX I Efficiency Check",
      "verification_method": "Temporal Delta Analysis",
      "required_confidence": 0.98
    },
    "GAX_II_COMPLIANCE": {
      "name": "GAX II Compliance Check",
      "verification_method": "Context Schema Verification",
      "required_confidence": 0.99
    },
    "GAX_III_INTEGRITY": {
      "name": "GAX III Integrity Check",
      "verification_method": "Source Hashing Validation (SHA-384)",
      "required_confidence": 1.0
    }
  },
  "artifact_registry": {
    "CSR": {
      "name": "Configuration Snapshot Receipt",
      "schema_version": "1.2",
      "artifact_class": "STATE_CONFIG",
      "integrity_target_id": "GAX_III_INTEGRITY"
    },
    "ECVM": {
      "name": "Execution Context Validation Map",
      "schema_version": "1.1",
      "artifact_class": "VALIDATION_MAP",
      "integrity_target_id": "GAX_II_COMPLIANCE"
    },
    "TEMM": {
      "name": "Transition Efficacy Measure",
      "schema_version": "2.1",
      "artifact_class": "METRIC_REPORT",
      "integrity_target_id": "GAX_I_EFFICIENCY"
    }
  },
  "stages": [
    {
      "stage_id": "S01",
      "actor_id": "CRoT_Agent",
      "operation_mandate": "Capture foundational execution state data from the control root.",
      "consumes": [],
      "produces": ["CSR"]
    },
    {
      "stage_id": "S07",
      "actor_id": "EMS",
      "operation_mandate": "Validate context against certified foundational state (CSR).",
      "consumes": [
        {"artifact_id": "CSR", "required_integrity": "GAX_III_INTEGRITY"}
      ],
      "produces": ["ECVM"]
    },
    {
      "stage_id": "S08",
      "actor_id": "EMS",
      "operation_mandate": "Measure operational deviation and efficacy of context transition using validated map.",
      "consumes": [
        {"artifact_id": "CSR", "required_integrity": "GAX_III_INTEGRITY"},
        {"artifact_id": "ECVM", "required_integrity": "GAX_II_COMPLIANCE"}
      ],
      "produces": ["TEMM"]
    },
    {
      "stage_id": "S11",
      "actor_id": "GAX_Executor",
      "operation_mandate": "Aggregate validated metrics (TEMM) and state data (CSR, ECVM) to execute finality resolution (P-01).",
      "consumes": [
        {"artifact_id": "CSR", "required_integrity": "GAX_III_INTEGRITY"},
        {"artifact_id": "ECVM", "required_integrity": "GAX_II_COMPLIANCE"},
        {"artifact_id": "TEMM", "required_integrity": "GAX_I_EFFICIENCY"}
      ],
      "produces": [],
      "finality_commitment": "P-01 Finality Commitment"
    }
  ]
}