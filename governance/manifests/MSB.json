{
  "manifestId": "MSB_V1.0",
  "description": "Metric Synthesis Blueprint: Defines the auditable formulas and weighted mappings utilized by L3 Synthesizers (RDM/SEM/SDR) to calculate S-01 (Efficacy) and S-02 (Risk) from core inputs (RCM, DSP-C, EDIS).",
  "schemaVersion": "GSEP-C V95.1",
  "metrics": {
    "S_01_Efficacy": {
      "formula": "(W_R * R_Score) + (W_P * P_Predict) - C_Overhead",
      "weights": {
        "W_R": 0.6, 
        "W_P": 0.4 
      },
      "inputs": ["RCM.ResourceUtility.score", "DSP-C.PredictionConfidence.scalar"]
    },
    "S_02_Risk": {
      "formula": "R_Vector + D_InputBias",
      "inputs": ["SPDM.SystemSafetyLimits.vector", "EDIS.ExternalSourceTrust.bias"]
    }
  },
  "parameters": {
    "R_Score_Normalization": "[0, 1]",
    "C_Overhead_Source": "RCM.Consumption.current"
  }
}