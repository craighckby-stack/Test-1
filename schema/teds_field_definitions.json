{
  "description": "Detailed schema definitions (type, format, constraints) for all mandatory TEDS fields, used by the IH Sentinel for deep structural validation.",
  "fields": {
    "timestamp": {
      "type": "string",
      "format": "ISO8601",
      "required_precision": "ms"
    },
    "system_id": {
      "type": "string",
      "pattern": "^[A-Z]{3}-[0-9]{4}-[A-Z0-9]{5}$"
    },
    "csr_lock_hash": {
      "type": "string",
      "format": "SHA256",
      "length": 64
    },
    "p01_result": {
      "type": "number",
      "range": [0.0, 1.0]
    },
    "environment_verified": {
      "type": "boolean"
    }
    /* ... all TEDS fields must be defined here ... */
  }
}