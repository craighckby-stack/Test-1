{  "schema_version": "v2.0",  "meta": {    "description": "Canonical and versioned definitions for all TEDS (Travel & Expense Data Structure) fields. Refactored for structured constraints and normalization pipelines.",    "last_updated": "2024-06-03T12:00:00Z",    "standard": "AGI/FieldDefinition/v3"  },  "config": {    "enforcement_mode": "STRICT",    "default_locale": "en-US",    "default_sensitivity": "GENERAL"  },  "shared_types": {    "TEDS_UUID": {      "type": "string",      "format": "uuid",      "description": "A globally unique identifier specific to this system."    },    "ISO_4217_CODE": {      "type": "string",      "pattern": "^[A-Z]{3}$",      "length": 3,      "description": "3-letter standard currency code."    },    "TEDS_USER_ID": {      "type": "string",      "pattern": "^U-[0-9a-f]{8}(-[0-9a-f]{4}){3}-[0-9a-f]{12}$",      "description": "Internal User Identifier."    }  },  "domains": {    "core_transaction": {      "TEDS_ID": {        "type": "TEDS_UUID",        "required": true,        "is_primary": true,        "sensitivity_level": "SYSTEM",        "description": "System-assigned unique transaction ID."      },      "transaction_date": {        "type": "string",        "format": "date-time",        "required": true,        "description": "The date and time the transaction occurred.",        "transformation_pipeline": [          {            "step": "standardize_timezone",            "params": {              "target": "UTC"            }          },          {            "step": "coerce_to_iso8601"          }        ]      },      "amount": {        "type": "number",        "required": true,        "sensitivity_level": "FINANCIAL",        "validation_rules": [          {            "rule": "minimum",            "value": 0,            "error_code": "E001_NEGATIVE"          },          {            "rule": "max_precision",            "value": 2          }        ],        "transformation_pipeline": [          {            "step": "round_to_decimals",            "params": {              "precision": 2            }          }        ]      },      "currency_code": {        "type": "ISO_4217_CODE",        "required": true,        "default": "USD"      }    },    "vendor": {      "vendor_name": {        "type": "string",        "required": true,        "validation_rules": [          {            "rule": "max_length",            "value": 255          }        ],        "transformation_pipeline": [          {            "step": "trim"          },          {            "step": "capitalize_words"          }        ]      },      "vendor_address_zip": {        "type": "string",        "required": false,        "description": "Vendor postal code.",        "validation_rules": [          {            "rule": "regex_match",            "pattern": "^[0-9]{5}(-[0-9]{4})?$",            "countries": [              "US"            ]          }        ]      }    }  }}