{
  "config_version": "2.0",
  "trust_protocol": "JWS_TPM_PROFILE",
  "verification_settings": {
    "key_path": "certs/tpm_signing_key.pem",
    "jwk_fetch_enabled": false,
    "integrity_policy_ref": "crit_crypto_policy.json" 
  },
  "provisioning": {
    "endpoint": "https://secure-root-of-trust.corp/api/v1/integrity/hashes",
    "manifest_id_template": "manifest_id={ID}",
    "expected_hash_field_template": "{ID}_HASH",
    "required_manifests": [
      "PVLM",
      "CFTM",
      "SBCM",
      "RTM"
    ]
  },
  "failover_strategy": {
    "mode": "ROM_BASELINE",
    "path": "/dev/rom/crit_baseline.sig"
  }
}