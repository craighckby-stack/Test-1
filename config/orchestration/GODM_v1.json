{
  "schemaVersion": "1.1",
  "deploymentId": "GODM_CORE_V1",
  "globalConfig": {
    "timeoutMinutes": 120,
    "failurePolicy": "STOP_ON_CRITICAL",
    "telemetryLevel": "Detailed"
  },
  "resourcePool": {
    "DB_Connector": {
      "type": "Postgres",
      "scope": "Global"
    },
    "Compute_Engine": {
      "type": "KubeCluster",
      "instanceType": "HighCPU"
    }
  },
  "pipeline": [
    {
      "stageId": "INGEST_DATA",
      "stageOrder": 1,
      "strategy": "SERIAL",
      "description": "Setup infrastructure required for data intake.",
      "resourceRefs": ["DB_Connector"],
      "dependencies": []
    },
    {
      "stageId": "PROVISION_COMPUTE",
      "stageOrder": 2,
      "strategy": "PARALLEL",
      "description": "Deploy core scalable compute environment.",
      "resourceRefs": ["Compute_Engine"],
      "dependencies": [
        {
          "targetId": "INGEST_DATA",
          "type": "HARD"
        }
      ]
    }
  ]
}