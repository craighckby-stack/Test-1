{
  "$schema": "../schema/mee.contract.schema.json",
  "name": "MEE_Core_Interface",
  "version": "1.1.0",
  "description": "Defines the contractual agreement and required capabilities for the Modular Execution Engine (MEE).",
  "metadata": {
    "stability": "GA",
    "required_engine_version": "^3.5.0"
  },
  "capabilities": {
    "transactions": true,
    "async_jobs": {
      "enabled": true,
      "queue_priority": "standard"
    }
  },
  "endpoints": [
    {
      "path": "/execute",
      "method": "POST",
      "required_auth": [
        "bearer",
        "api_key"
      ],
      "rate_limit_group": "high_throughput"
    },
    {
      "path": "/status/{id}",
      "method": "GET",
      "required_auth": [
        "api_key"
      ]
    }
  ],
  "timeouts_ms": {
    "default_request": 3000,
    "transaction_commit": 15000
  }
}