/**
 * @class
 *
 * @property {Map<string, Function>} _listeners - Listeners map
 * @property {string} _status - Application status
 */
class Lifecycle {
  /**
   * @property {string} _status - Application status
   */
  static #status;

  /**
   * @static
   * @readonly
   * @returns {string}
   */
  static get status() {
    return this.#status;
  }

  /**
   * @static
   * @param {string} value
   */
  static set status(value) {
    this.#status = value;
  }

  /**
   * @static
   * @async
   * @param {string} event
   * @param {...any} args
   */
  static async applyLifecycle(event, ...args) {
    try {
      const listeners = this.#_listeners.get(event);
      if (listeners) {
        await Promise.all(listeners.map((listener) => listener(...args)));
      }
    } catch (error) {
      console.error('Lifecycle error:', error);
    }
  }

  /**
   * @static
   * @readonly
   * @returns {Map<string, Function>}
   */
  static get _listeners() {
    return new Map();
  }

  /**
   * @static
   * @param {Map<string, Function>} value
   */
  static set _listeners(value) {
    this.#_listeners = value;
  }

  /**
   * @static
   * @async
   * @param {string} event
   * @param {...any} config
   */
  static async configure(event, config) {
    const listeners = this.#_listeners.get(event);
    if (listeners) {
      await Promise.all(listeners.map((listener) => listener(config)));
    }
  }

  /**
   * @static
   * @readonly
   * @returns {string[]}
   */
  static get lifecycleMethods() {
    return ['configured', 'loaded', 'shuttingDown'];
  }
}

class NexusCore extends Lifecycle {
  /**
   * @property {LifecycleEventManager} context - Lifecycle event manager
   */
  #context;

  /**
   * @property {string} status - Application status
   */
  #status;

  /**
