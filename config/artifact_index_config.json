   * @param {string} event
   * @param {...any} config
   */
  async shutdownDependencies(event, config) {
    try {
      await this.context.emit(event, config);
    } catch (error) {
      console.error('Shut down dependencies error:', error);
    }
  }

  /**
   * @async
   * @param {string} event
   * @param {...any} config
   */
  async loadingDependenciesComplete(event, config) {
    try {
      await this.context.emit(event, config);
