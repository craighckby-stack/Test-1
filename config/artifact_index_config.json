   */
  set status(value) {
    this.#status = value;
  }

  /**
   * @async
   */
  async reload() {
    try {
      await this.context.emit('RELOAD');
    } catch (error) {
      console.error('Reload error:', error);
    }
  }

  /**
   * @async
   * @param {Config} config
   */
  async applyConfig(config) {
    try {
      await super.applyLifecycle('CONFIGURE', config);
    } catch (error) {
      console.error('CONFIGURE method failed:', error);
    }
  }

  /**
   * @async
   * @param {Config} config
   */
  async validateConfig(config) {
    try {
      await Config.validate(config);
    } catch (error) {
      console.error('VALIDATE_CONFIG method failed:', error);
    }
  }

  /**
   * @async
   */
  async load() {
    const startTime = performance.now();
    try {
      console.log('Loading...');
      await new Promise((resolve) => setTimeout(resolve, 1000));
      console.log('Loading complete...');
      this.status = 'LOADING';
      await new Promise((resolve) => setTimeout(resolve, 100));
      console.log(`Loaded in ${performance.now() - startTime}ms`);
    } catch (e) {
      console.error('Load error:', e);
    }
  }

  /**
   * @async
   */
  async start() {
    try {
      const startTime = performance.now();
      await Promise.all(
        Lifecycle.lifecycleMethods().map(async (methodName) => {
          if (this[methodName] instanceof Function) {
            await this[methodName](startTime);
          }
        })
      );
      console.log('Lifecycle started in ' + (performance.now() - startTime) + 'ms');
      this.status = 'STARTED';
    } catch (error) {
      console.error('START method failed:', error);
    }
  }

  /**
   * @async
   */
  async destroy() {
    this.status = 'DESTROYED';
    this.context.dispose();
    this.status = 'LOADED';
  }

  /**
   * @async
   * @param {string} event
   * @param {Function} handler
   */
  async subscribe(event, handler) {
    await this.context.subscribe(event, handler);
  }

  /**
   * @async
   * @param {string} event
   * @param {Function} handler
   */
  async unsubscribe(event, handler) {
    await this.context.unsubscribe(event, handler);
  }

  /**
   * @async
   * @param {string} event
   * @param {...any} args
   */
  async emit(event, ...args) {
    return new Promise((resolve, reject) => {
      this.context.emit(event, ...args, (err) => {
        if (err) {
          reject(err);
        } else {
          resolve();
        }
      });
    });
  }

  /**
   * @async
   * @param {string} event
   * @param {...any} config
   */
  async initLoadingDep(event, config) {
    try {
      await this.context.emit(event, config);
    } catch (error) {
      console.error('Init loading dep error:', error);
    }
  }

  /**
