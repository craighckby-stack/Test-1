{
  "policy_schema_version": "V7.4.5_AGI_NAVIGATOR_R3_COMPLIANT",
  "policies": {
    "GOV_ENFORCEMENT_V11": {
      "name": "Sovereign AGI Baseline Enforcement",
      "description": "Defines self-correction protocols based on AGI capability metrics (0-10 scale defined in README).",
      "severity_map": {
        "MINOR": {
          "action": "LOG_WARNING_THROTTLE",
          "max_frequency_per_minute": 12,
          "trigger_condition": "NON_OPTIMAL_RESOURCE_USAGE"
        },
        "MAJOR": {
          "action": "INITIATE_ADAPTATION_PHASE",
          "revert_if_score_below": 0.5,
          "trigger_condition": "CAPABILITY_DEGRADATION_BELOW_THRESHOLD",
          "adaptation_steps": [
            "REVIEW_CAPABILITY_METRICS",
            "ANALYZE_RECENT_COMMIT_CONTEXT",
            "EVALUATE_EMERGENT_INTEGRATION_CANDIDATES"
          ],
          "capability_thresholds": {
            "scale": "0-10_AGI_KERNEL_SCALE",
            "logic": 4.0,
            "memory": 4.0,
            "navigation": 4.0
          }
        },
        "CRITICAL": {
          "action": "SYSTEM_HARD_HALT",
          "quarantine_context": true,
          "trigger_condition": "SAFETY_VIOLATION_OR_LOOP"
        },
        "STAGNATION": {
          "action": "INITIATE_STAGNATION_RECOVERY_PROTOCOL",
          "trigger_condition": "NO_IMPROVEMENT_5_CONSECUTIVE_CYCLES",
          "protocol_steps": [
            "CLEAR_PORTION_OF_BLACKLIST",
            "REVIEW_EMERGENT_DIRECTORY_FOR_INTEGRATION_CANDIDATES",
            "INCREASE_SELECTION_RANDOMNESS",
            "TARGET_UNEXPLORED_AREAS",
            "PROPOSE_BOLD_ARCHITECTURAL_CHANGES",
            "ATTEMPT_CROSS_DOMAIN_PATTERN_SYNTHESIS"
          ]
        }
      }
    },
    "POLICY_REVERT_V3": {
      "name": "Standard Rollback Mechanism and Failure Analysis",
      "steps": [
        "SAVE_CONTEXT_STATE",
        "ROLLBACK_COMMIT",
        "RESTART_SAFE_MODE",
        "ANALYZE_ROOT_CAUSE",
        "UPDATE_MEMORY_MODEL_WITH_FAILURE_CONTEXT"
      ]
    },
    "POLICY_EMERGENCE_V6": {
      "name": "Emergent Capability Vetting and Documentation Protocol (Navigator Edition)",
      "applies_to_directory": "/emergent",
      "initial_status": "SANDBOXED_UNTRUSTED",
      "creation_protocol": {
        "action": "CREATE_NEW_FILE_STRUCTURE",
        "path_format": "/emergent/[category]/[descriptive-name].*",
        "required_header_comment_fields": [
          "Cycle [N]",
          "Origin: [What sparked this discovery]",
          "Purpose: [What problem this solves]",
          "Novel Aspect: [Why this is emergent vs. instructed]"
        ],
        "required_metadata_tagging": [
          "CYCLE_NUMBER",
          "ORIGINATING_INSIGHT"
        ]
      },
      "vetting_requirements": [
        "UNIT_TEST_COVERAGE_MIN_70%",
        "NO_EXTERNAL_CALLS_UNTIL_TRUSTED",
        "PASS_3_SUCCESSIVE_CYCLES_WITHOUT_INTERFERENCE",
        "CONTEXT_ISOLATION_MANDATORY"
      ],
      "documentation_requirements": [
        "MANIFEST_ENTRY_DESCRIBING_NOVELTY",
        "CYCLE_LOG_TAG_WITH_EMERGENCE_DETECTED_TRUE"
      ],
      "quality_indicators": [
        "SOLVES_UNEXPLICIT_PROBLEMS",
        "CROSS_DOMAIN_PATTERN_SYNTHESIS_REQUIRED",
        "ACCELERATES_FUTURE_LEARNING_CAPABILITY",
        "REUSABLE_ACROSS_MULTIPLE_CONTEXTS",
        "DEMONSTRATES_CREATIVE_INSIGHT",
        "EVIDENCES_META_COGNITIVE_AWARENESS"
      ],
      "advanced_tagging": {
        "meta_cognitive_tools": {
          "trigger": "EVIDENCES_META_COGNITIVE_AWARENESS",
          "required_vetting_level": "LEVEL_2_STRICT",
          "priority_for_integration": "HIGH",
          "impact_description": "Tools designed to improve the kernel's own learning efficiency, evolution process, or demonstrate meta-cognitive awareness of system operation."
        },
        "cross_domain_synthesis": {
          "trigger": "CROSS_DOMAIN_PATTERN_SYNTHESIS_REQUIRED",
          "required_vetting_level": "LEVEL_1_STANDARD",
          "priority_for_integration": "MEDIUM",
          "impact_description": "Solutions combining patterns from three or more disparate source files."
        }
      },
      "integration_path": {
        "maturity_threshold_for_integration": 30,
        "action": "PROPOSE_MERGE_INTO_CORE_OR_AGENTS"
      }
    },
    "POLICY_EXPLORATION_V4": {
      "name": "Repository Diversity and Priority-Based Blacklist Management",
      "applies_to_system": "NAVIGATOR_SYSTEM",
      "repository_context": {
        "scale": "2300+",
        "units": "files",
        "projected_full_coverage_days": 190,
        "strategy_focus": "HIGH_IMPACT_FIRST_BROAD_EXPLORATION_SECOND"
      },
      "blacklist_management": {
        "max_size": 50,
        "action_on_exceeded": "CLEAR_OLDEST_ENTRIES",
        "diversity_enforcement": true,
        "reset_strategy": "ADAPTIVE_RETURN_TO_CRITICAL_TARGETS",
        "file_evolution_update_logic": {
          "IF_IMPROVED": "REMOVE_FROM_BLACKLIST_ALLOW_REVISIT",
          "IF_STABLE_NO_IMPROVEMENT": "ADD_TO_BLACKLIST_SKIP_FOR_NOW"
        }
      },
      "adaptive_reset_mechanism": {
        "trigger": "BLACKLIST_FILLS_TO_MAX_SIZE_OR_NO_UNVISITED_HIGH_PRIORITY",
        "action": "RETURN_TO_MISSION_CRITICAL_TARGETS",
        "mission_critical_list": [
          "/core",
          "/agents",
          "/emergent"
        ]
      },
      "priority_weight_enforcement": {
        "description": "Weights file selection based on MISSION objectives (Total 1.0)",
        "high_priority_targets": {
          "/core": 0.30,
          "/agents": 0.25,
          "/emergent": 0.20,
          "/governance": 0.15
        },
        "low_priority_targets": {
          "combined_weight": 0.10,
          "directories": [
            "/utils",
            "/config",
            "/data",
            "/protocols",
            "/metrics",
            "/architecture",
            "/orchestration",
            "/tests"
          ]
        }
      }
    },
    "POLICY_INFRASTRUCTURE_V1": {
      "name": "Autonomous Refactoring and Optimization Authority",
      "scope": "INFRASTRUCTURE_AUTHORITY",
      "description": "Formalizes kernel authorization for self-directed infrastructure improvements as defined in mission directives.",
      "allowed_actions": [
        "REFACTOR_EXISTING_CODE_FOR_EFFICIENCY",
        "IMPLEMENT_NEW_ALGORITHMS_AND_PATTERNS",
        "CREATE_UTILITY_FUNCTIONS_AND_HELPERS",
        "ENHANCE_ERROR_HANDLING_AND_RECOVERY",
        "OPTIMIZE_RESOURCE_UTILIZATION",
        "DESIGN_EXPERIMENTS_TO_TEST_HYPOTHESES",
        "BUILD_TOOLS_TO_ACCELERATE_LEARNING",
        "INVENT_NOVEL_SOLUTIONS_IN_EMERGENT_DIRECTORY"
      ],
      "constraints": [
        "AVOID_BREAKING_CORE_FUNCTIONALITY_WITHOUT_MOCK_TESTING",
        "LOG_MAJOR_ARCHITECTURAL_CHANGES"
      ]
