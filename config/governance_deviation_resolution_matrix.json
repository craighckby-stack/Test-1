{
  "metadata": {
    "version": "v2.0",
    "updated_by": "Sovereign AGI v94.1",
    "description": "Optimized hierarchical matrix mapping deviation context to structured resolution protocols, replacing flat arrays with indexed policies."
  },
  "protocols": {
    "P001_CRITICAL_ISOLATION": {
      "strategy": "ImmediateIsolation",
      "action": "HardHalt",
      "timeout_ms": 50,
      "report_level": 9,
      "fallback": "Trigger_Manual_Override"
    },
    "P002_HIGH_SELF_CORRECT": {
      "strategy": "AutomatedCorrection",
      "action": "InjectAdaptivePatch",
      "timeout_ms": 5000,
      "report_level": 7,
      "fallback": "Log_And_Retry_Once"
    },
    "P003_MINOR_OBSERVE": {
      "strategy": "PassiveObservation",
      "action": "LogWarning",
      "timeout_ms": 30000,
      "report_level": 3,
      "fallback": "None"
    }
  },
  "deviation_rules": [
    {
      "id": "R_D_CORE_DRIFT",
      "priority_rank": 950,
      "match_condition": {
        "severity": ["CRITICAL"],
        "type_regex": ".*(IntegrityViolation|UnauthorizedMutation).*",
        "source_tag": ["CORE", "GOVERNANCE"]
      },
      "resolution": {
        "protocol_id": "P001_CRITICAL_ISOLATION",
        "authorized_entity": "SentinelGuard"
      }
    },
    {
      "id": "R_D_CONFIG_ERR",
      "priority_rank": 500,
      "match_condition": {
        "severity": ["HIGH", "MEDIUM"],
        "type_regex": "^Config.*Mismatch$",
        "source_tag": ["CONFIGURATION_SERVICE"]
      },
      "resolution": {
        "protocol_id": "P002_HIGH_SELF_CORRECT",
        "authorized_entity": "Governance_Monitor"
      }
    }
  ]
}