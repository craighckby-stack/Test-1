{
  "schema_version": "v94.1",
  "analysis_thresholds": {
    "MPAM_deviation_max": 0.05, 
    "SGS_integrity_score_min": 0.98,
    "ADTM_anomaly_count_max": 3
  },
  "action_map": [
    {
      "condition": "MPAM_deviation_max_exceeded OR ADTM_anomaly_count_count_max_exceeded",
      "output": "RRP_Authorization_Level_3",
      "description": "Severe state corruption detected, mandatory full rollback."
    },
    {
      "condition": "SGS_integrity_score_min_failed",
      "output": "IH_LOCKDOWN_AUDIT",
      "description": "Integrity breach confirmed, requiring external human governance audit before RRP authorization."
    },
    {
      "condition": "NOT(MPAM_deviation_max_exceeded OR SGS_integrity_score_min_failed OR ADTM_anomaly_count_max_exceeded)",
      "output": "FALSE_POSITIVE_RCA_COMPLETE",
      "description": "Telemetry inconsistent with critical failure, authorizing localized state repair instead of RRP."
    }
  ]
}