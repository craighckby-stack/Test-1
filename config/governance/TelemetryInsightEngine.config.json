{
  "service_id": "TIE_01",
  "description": "Configuration for the Telemetry Insight Engine (TIE), responsible for anomaly detection and P3 correlation against Certified Operational Telemetry.",
  "input_stream": "telemetry.operational.certified",
  "output_stream": "audit.alerts.performance",
  "correlation_window_ms": 300000,
  "anomaly_detection_rules": [
    {
      "metric": "rql_metrics.cache_misses",
      "threshold_factor": 3.0, 
      "baseline_period_hours": 24,
      "severity": "P3_CRITICAL"
    },
    {
      "metric": "resources.latency_ms",
      "threshold_factor": 1.5,
      "condition": "agent_source starts with GAX",
      "severity": "P4_WARNING"
    }
  ],
  "context_hash_index_ttl_minutes": 60
}