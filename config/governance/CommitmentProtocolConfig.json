{
  "protocol_name": "GSEP-C: Governance State Evolution Protocol - Commitment Phase",
  "version": "V94.1",
  "cryptography_standards": {
    "manifest_hashing_algorithm": "SHA3-256",
    "signature_algorithm": "Ed25519",
    "data_encoding": "Base64URL"
  },
  "required_agents": {
    "SGS": {
      "role": "State Proposal/Initiator",
      "required_attestation": true
    },
    "GAX": {
      "role": "Constraint Audit/Veto Power",
      "required_attestation": true
    },
    "CRoT": {
      "role": "Final Commitment Registrar",
      "required_attestation": true
    }
  },
  "audit_parameters": {
    "minimum_utility_margin_epsilon": 0.001,
    "max_acceptable_signature_latency_ms": 500
  }
}