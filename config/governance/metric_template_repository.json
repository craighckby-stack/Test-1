{
  "description": "Standardized templates for Metric Synthesis Bank (MSB) operations (L5). Ensures consistent, pre-validated quantification of S-01 (Efficacy) and S-02 (Risk) based on SST Classification.",
  "metadata": {
    "version": "MTR V1.0",
    "governance_rule": "MEC Governance Rules reference"
  },
  "templates": [
    {
      "sst_classification": "UTILITY_UPDATE",
      "template_id": "T001_PERFORMANCE_TUNE",
      "required_inputs": ["P_Latency_Current", "P_Latency_Projected", "R_Dependency_Change_Magnitude"],
      "s_01_derivation": "P_Latency_Current - P_Latency_Projected",
      "s_02_derivation": "Weighted_R_Factor * R_Dependency_Change_Magnitude",
      "default_weighted_r_factor": 0.05
    },
    {
      "sst_classification": "DATA_MODEL_SCHEMA_CHANGE",
      "template_id": "T002_SCHEMA_MIGRATION",
      "required_inputs": ["R_Schema_Compatibility_Score", "R_Data_Loss_Risk"],
      "s_01_derivation": "Complexity_Reduction_Score",
      "s_02_derivation": "R_Data_Loss_Risk + (1 - R_Schema_Compatibility_Score)"
    }
  ]
}