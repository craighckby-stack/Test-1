{
  "version": "94.1.1",
  "map_id": "GOV_LGM_AETHELRED_941A",
  "description": "Adaptive Execution Targeting Layer (AETL) rules for engine selection based on input context severity, scope, and resource constraints.",
  "meta": {
    "engine_schema_version": "3.0",
    "updated_by": "Sovereign AGI v94.1"
  },
  "default_behavior": {
    "engine_target": "engines/core/consensus_algorithm_v3",
    "priority_level": 5,
    "failure_policy": "RETRY_DEFAULT"
  },
  "mapping_rules": [
    {
      "id": "CRITICAL_SECURITY_EVENT",
      "priority": 100,
      "scope": "Global",
      "context_match_criteria": {
        "severity": ["CRITICAL", "EMERGENCY"],
        "origin_path": "/security/",
        "system_load": {
          "above": 0.9
        }
      },
      "engine_target": "engines/security/rapid_mitigation_v94_1",
      "failure_policy": "QUEUE_FOR_HUMAN_REVIEW"
    },
    {
      "id": "STANDARD_EVOLUTION_REQUEST",
      "priority": 70,
      "scope": "Evolution",
      "context_match_criteria": {
        "task_type": ["Refactor", "Scaffold"],
        "dependency_delta": {
          "lt": 5
        }
      },
      "engine_target": "engines/evolution/autonomous_code_evolution_v5_4",
      "failure_policy": "FALLBACK_TO_DEFAULT"
    },
    {
      "id": "LOW_PRIORITY_MAINTENANCE",
      "priority": 40,
      "scope": "Maintenance",
      "context_match_criteria": {
        "severity": ["LOW", "INFO"],
        "system_load": {
          "below": 0.5
        },
        "time_window": "OFF_PEAK"
      },
      "engine_target": "engines/utility/metadata_optimization_v1",
      "failure_policy": "SILENT_DROP"
    }
  ]
}