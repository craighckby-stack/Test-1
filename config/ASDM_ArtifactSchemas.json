{
  "description": "Artifact Schema Definition Module (ASDM) V1.0: Mandatory structural schemas for all GSEP cryptographic commitments, ensuring AICV integrity validation against predefined versioned formats.",
  "schemas": {
    "PolicyDefinition_EPDP": {
      "version": "1.1",
      "type": "object",
      "properties": {
        "intent_id": {"type": "string"},
        "rsam_veto_flag": {"type": "boolean"},
        "violation_ruleset": {"type": "array"},
        "commit_timestamp": {"type": "integer"}
      },
      "required": ["intent_id", "rsam_veto_flag"]
    },
    "ProofManifestHash_PMH_Lock": {
      "version": "1.0",
      "type": "object",
      "properties": {
        "proof_hash_alg": {"type": "string"},
        "proof_root_hash": {"type": "string"},
        "s01_efficacy_score": {"type": "number"},
        "psr_trace_metadata": {"type": "object"}
      },
      "required": ["proof_root_hash", "s01_efficacy_score"]
    },
    "DecisionalStateCheckpoint_DSCM_Lock": {
      "version": "1.2",
      "type": "object",
      "properties": {
        "gsep_stage": {"type": "integer"},
        "s01_trust": {"type": "number"},
        "s02_risk": {"type": "number"},
        "s03_veto": {"type": "boolean"},
        "p01_pass": {"type": "boolean"},
        "parent_artifact_hash": {"type": "string"}
      },
      "required": ["s01_trust", "s02_risk", "s03_veto", "p01_pass"]
    },
    "AIA_Ledger_Entry_D01": {
      "version": "1.0",
      "type": "object",
      "properties": {
        "transaction_id": {"type": "string"},
        "mcr_version_lock": {"type": "string"},
        "parent_dscm_hash": {"type": "string"},
        "ama_trace_final_hash": {"type": "string"}
      },
      "required": ["transaction_id", "mcr_version_lock"]
    }
  }
}