{
  "system_context": "AXEL_R9_GAX_PROD",
  "version": "94.1.0",
  "integrity_check": {
    "enabled": true,
    "hashing_algorithm": "SHA3-512",
    "config_ttl_seconds": 3600
  },
  "resource_management": {
    "policy_name": "AdaptiveV94",
    "worker_pool": {
      "min_workers": 4,
      "max_workers": 64,
      "dynamic_scaling_enabled": true,
      "utilization_threshold": 0.85
    },
    "memory_allocation": {
      "process_max_mb": 8192,
      "cache_ratio": 0.35,
      "governance_mode": "EFFICIENT"
    }
  },
  "logging_config": {
    "default_level": "WARNING",
    "transports": [
      {"type": "console", "level": "INFO", "format": "simple"},
      {"type": "file", "level": "DEBUG", "path": "/var/log/axel/runtime.log"}
    ]
  },
  "core_services": [
    {"name": "Scheduler_V9", "criticality": "HIGH", "dependencies": []},
    {"name": "IOHandler_V7", "criticality": "MEDIUM", "dependencies": ["Scheduler_V9"]},
    {"name": "TelemetryBus", "criticality": "LOW", "dependencies": []}
  ]
}