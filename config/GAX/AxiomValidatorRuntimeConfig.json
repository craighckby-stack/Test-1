class NexusCore {
  #lifecycle = {
    configured: false,
    loaded: false,
    shuttingDown: false,
    bootstrapped: false,
  };

  #status = "INIT";

  get status() {
    return this.#status;
  }

  set status(value) {
    this.#status = value;
    const currentValue = this.#status;
    const lifecycle = this.#lifecycle;
    if (value !== 'INIT') {
      console.log(`NexusCore instance is ${value}.`);
      if (value === 'SHUTDOWN') {
        // prevent shutdown hook re-execution
        lifecycle.shuttingDown = false;
        lifecycle.load = () => {};
        lifecycle.shutdown = () => {};
      }
    }
    if (currentValue === 'INIT' && value !== 'INIT') {
      lifecycle.bootstrapped = true;
    }
  }

  get lifecycle() {
    return this.#lifecycle;
  }

  synergyManager = null;

  get synergy() {
