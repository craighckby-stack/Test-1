{
  "metadata": {
    "schema_version": "2.1.0",
    "name": "Global_Dimensional_Velocity_Model",
    "description": "Standardized configuration defining feature space topology, real-time change monitoring (velocity), and systemic data governance rules."
  },
  "core_model_dimensionality": {
    "feature_dimensions": 768,
    "quantization_profile": {
      "type": "Product_Quantization",
      "bits_per_centroid": 8
    },
    "storage_engine_config": {
      "default_metric": "cosine",
      "strategy": "HNSW",
      "parameters": {
        "M": 32,
        "ef_construction": 200,
        "reindex_trigger_load": 0.85
      }
    }
  },
  "runtime_velocity_metrics": {
    "normalization_window_ms": 3600000,
    "anomaly_thresholds": {
      "relative_std_dev_multiplier": 3.0,
      "min_change_percentage": 0.05
    },
    "rate_limiting": {
      "debounce_window_ms": 500,
      "max_events_per_second": 1000,
      "dynamic_throttle_enabled": true
    },
    "event_retention_days": 7
  },
  "data_governance_policies": {
    "auto_pruning": {
      "enabled": true,
      "stale_age_days": 30
    },
    "schema_validation": {
      "enforcement_level": "STRICT_WARN",
      "allowed_levels": ["OFF", "WARN", "STRICT_WARN", "BLOCK"],
      "integrity_check_frequency_hours": 6
    }
  }
}