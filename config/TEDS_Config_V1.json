{
  "schema_version": "TEDS_V1.0",
  "integrity_model": "HashChain: SHA-256",
  "mandatory_fields": [
    "Timestamp_UTC",
    "Event_ID",
    "Agent_Source",
    "DSE_Cycle_N",
    "GSEP_C_Stage_SXX",
    "Severity_Level",
    "P01_Context_Hash",
    "Payload_Data_Type",
    "Artifact_Reference_IDs"
  ],
  "chain_link_enforcement": {
    "required": true,
    "hash_target": "Payload_Data + Previous_Block_Hash + Timestamp_UTC",
    "frequency": "Per-Event"
  },
  "access_control": {
    "write": "Exclusive_CRoT_SGS_Only",
    "read": "All_Agents_Post_S12"
  }
}