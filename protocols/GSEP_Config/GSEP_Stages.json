{
  "version": "1.0",
  "description": "Defines Governance Stage Execution Profiles (GSEP Stages), specifying integrity thresholds and required context synchronization states necessary for GFRM action targets.",
  "stages": {
    "S_01_INPUT_VLD": {
      "name": "Input Validation Stage",
      "integrityThreshold": 0.999,
      "requiredContextKeys": ["schema_validation_hash", "agent_authorization_state"],
      "failoverActionReference": "GFRM:HALT_PROTOCOL"
    },
    "S_03_CONTEXT_SYNC": {
      "name": "Context Synchronization Stage",
      "integrityThreshold": 0.95,
      "requiredContextKeys": ["environmental_data_hash", "active_SST_ID"],
      "failoverActionReference": "GFRM:INIT_ROLLBACK_SST_PREV"
    },
    "S_04_METRIC_DYN": {
      "name": "Dynamic Metric Derivation Stage",
      "integrityThreshold": 0.85,
      "requiredContextKeys": ["current_viability_score", "proposed_recovery_path"],
      "failoverActionReference": "GFRM:TRANSITION_S_04_VIABILITY"
    }
  }
}