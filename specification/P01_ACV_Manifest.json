{
  "protocol_version": "V97.1",
  "description": "Mandatory schema definition for the Axiomatic State Manifest (ASM) required for P-01 validation at GSEP-C Stage S11.",
  "checkpoint_id": "P-01",
  "target_agent": "GAX",
  "schema_keys_required": [
    "TEMM", "UFRM", "CFTM",
    "ECVM",
    "PVLM", "MPAM", "ADTM"
  ],
  "axiomatic_constraints": {
    "UMA_I": {
      "type": "Comparison",
      "metrics": ["TEMM", "UFRM", "CFTM"],
      "validation": "TEMM >= UFRM + CFTM"
    },
    "CA_II": {
      "type": "Boolean_Check",
      "metrics": ["ECVM"],
      "validation": "ECVM == True"
    },
    "AI_III": {
      "type": "Integrity_Logic_Veto",
      "metrics": ["PVLM", "MPAM", "ADTM"],
      "validation": "! (PVLM || MPAM || ADTM)"
    }
  }
}